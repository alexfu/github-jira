{
  "name": "github-jira",
  "version": "1.2.0",
  "author": "Alex Fu @alexfu",
  "bin": {
    "github-jira": "./bin/run"
  },
  "bugs": "https://github.com/alexfu/mynewcli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.6",
    "@oclif/config": "^1.9.0",
    "@oclif/plugin-help": "^2.1.4",
    "@types/inquirer": "^6.0.0",
    "@types/nodegit": "^0.22.4",
    "axios": "^0.19.0",
    "cli-ux": "^5.2.0",
    "inquirer": "^6.2.2",
    "nodegit": "^0.26.4",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.0",
    "@oclif/test": "^1.2.2",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.7",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/alexfu/github-jira",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "macos": {
      "identifier": "com.alexfu.github-jira"
    },
    "commands": "./lib/commands",
    "bin": "github-jira",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "alexfu/github-jira",
  "scripts": {
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "postpack": "rm -f oclif.manifest.json",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
